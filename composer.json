{
    "name": "imscp/roundcube-plugin-installer",
    "description": "i-MSCP composer-installer for Roundcube plugins.",
    "type": "composer-installer",
    "license": "GPL-3.0+",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Thomas Bruederli",
            "email": "thomas@roundcube.net"
        },
        {
            "name": "Till Klampaeckel",
            "email": "till@php.net"
        },
        {
            "name": "Laurent Declercq",
            "email": "l.declercq@nuxwin.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Roundcube\\Composer": "src/"
        }
    },
    "extra": {
        "class": "Roundcube\\Composer\\PluginInstaller"
    },
    "bin": [
        "src/bin/rcubeinitdb.sh"
    ],
    "require": {
        "php": ">=5.3.0",
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "composer/composer": "^1.0.0-alpha11"
    },
    "replace": {
        "roundcube/plugin-installer": "0.1.*"
    }
}
